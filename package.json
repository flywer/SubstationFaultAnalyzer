{
  "name": "electron-app-template",
  "version": "0.1.2",
  "description": "A template used by the electron framework",
  "main": "dist/main/index.js",
  "scripts": {
    "dev": "chcp 65001 && rimraf dist && vite",
    "build": "rimraf dist && vite build",
    "fix": "electron-fix start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder uninstall-app-deps",
    "rebuild": "electron-rebuild"
  },
  "dependencies": {
    "crypto-ts": "^1.0.2",
    "einf": "^1.3.0",
    "electron-log": "^4.4.8",
    "electron-updater": "^5.3.0",
    "exceljs": "^4.3.0",
    "jsonfile": "^6.1.0",
    "lodash-es": "^4.17.21",
    "pinia": "2.1.4",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.1.6",
    "typeorm": "0.3.17",
    "vue": "^3.2.38"
  },
  "devDependencies": {
    "@ant-design/icons-vue": "^6.1.0",
    "@lightwing/eslint-config": "0.0.7",
    "@types/lodash-es": "^4.17.6",
    "@vicons/antd": "^0.12.0",
    "@vicons/carbon": "^0.12.0",
    "@vicons/fa": "^0.12.0",
    "@vicons/fluent": "^0.12.0",
    "@vicons/ionicons5": "^0.12.0",
    "@vicons/material": "^0.12.0",
    "@vicons/tabler": "^0.12.0",
    "@vitejs/plugin-vue": "3.1.0",
    "@vue/compiler-sfc": "3.2.39",
    "@vue/test-utils": "2.0.2",
    "ant-design-vue": "^3.2.12",
    "autoprefixer": "^10.4.16",
    "electron": "20.0.0",
    "electron-builder": "23.3.3",
    "electron-debug": "^3.2.0",
    "electron-devtools-installer": "^3.2.0",
    "electron-fix": "^1.1.3",
    "electron-rebuild": "^3.2.9",
    "eslint": "8.22.0",
    "less": "^4.1.3",
    "less-loader": "^11.0.0",
    "lint-staged": "13.0.3",
    "naive-ui": "^2.34.3",
    "parse-json": "^6.0.2",
    "postcss": "^8.4.31",
    "rimraf": "3.0.2",
    "simple-git-hooks": "2.8.0",
    "tailwindcss": "^3.3.3",
    "tslib": "2.4.0",
    "typescript": "4.8.3",
    "unplugin-vue-define-options": "^1.2.3",
    "vite": "3.1.3",
    "vite-plugin-doubleshot": "0.0.6",
    "vite-plugin-top-level-await": "^1.3.1",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vitest": "0.23.4",
    "vue-router": "~4.0.x",
    "vue-tsc": "0.40.13"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue,md,json,yml}": [
      "eslint --fix"
    ]
  }
}
